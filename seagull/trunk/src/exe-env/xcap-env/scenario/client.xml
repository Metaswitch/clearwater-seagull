<?xml version="1.0" encoding="ISO-8859-1" ?>
<scenario>

<traffic>
   <send channel="channel-1">
   <action>
     <!-- open a socket for each call -->
     <open args="mode=client;dest=127.0.0.1:8080"></open>
   </action>
     <message>
     <!-- header -->
     <! [ CDATA[GET  http://xcap.example.com/address-book/users/petri/adbook1/address-book/entry/street/@paved  HTTP/1.1]]>
     <!-- body --> 
     <![CDATA[<?xml version="1.0" encoding="UTF-8"?>
              <list name="close-friends">
                <entry uri="sip:joe@example.com">
                    <display-name>Joe Smith</display-name>
                </entry>
                <entry uri="sip:nancy@example.com">
                    <display-name>Nancy Gross</display-name>
                </entry>
                <entry uri="sip:petri@example.com">
                    <display-name>Petri Aukia</display-name>
                </entry>
             </list>]]>
     </message>
   </send>

   <receive channel="channel-1">
    <message>
     <!-- header -->
     <![CDATA[HTTP/1.1 200 OK
              Content-Type: application/xml-attribute-value
              Content-Length: 4]]>
     <!-- body --> 
     <![CDATA[true]]>
    </message>

   <action>
     <!-- close the socket -->
     <close></close>
   </action>
   </receive>
</traffic>

</scenario>









